<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.umich.umd.obdpractice"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dg_tech"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.umich.umd.obdpractice.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.umich.umd.obdpractice.DisplayMessageActivity"
            android:label="@string/title_activity_display_message"
            android:parentActivityName="com.umich.umd.obdpractice.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.umich.umd.obdpractice.MainActivity" />
        </activity>
        <activity
            android:name="com.umich.umd.obdpractice.NetworkSetupActivity"
            android:label="@string/title_activity_network_setup"
            android:parentActivityName="com.umich.umd.obdpractice.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.umich.umd.obdpractice.MainActivity" />
        </activity>
        <activity
            android:name="com.umich.umd.obdpractice.LogFilesList"
            android:label="@string/title_activity_log_files_list"
            android:parentActivityName="android.app.ListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="android.app.ListActivity" />
        </activity>
        <activity
            android:name="com.umich.umd.obdpractice.ViewStoredLogs"
            android:label="@string/title_activity_view_stored_logs"
            android:parentActivityName="com.umich.umd.obdpractice.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.umich.umd.obdpractice.MainActivity" />
        </activity>
        <activity
            android:name="com.umich.umd.obdpractice.ViewStoredLogsActivity"
            android:label="@string/title_activity_view_stored_logs"
            android:parentActivityName="com.umich.umd.obdpractice.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.umich.umd.obdpractice.MainActivity" />
        </activity>
    </application>

</manifest>